		"typescript": "^2.9.2"
	},
	"devDependencies": {
		"@types/chai": "^4.1.4",
		"@types/graphlib": "^2.1.4",
		"@types/lodash": "^4.14.112",
		"@types/mocha": "^5.2.4",
		"@types/node": "^10.5.2",
		"chai": "^4.1.2",
		"mocha": "^5.2.0",
		"ts-node": "^7.0.0"
	}
}
